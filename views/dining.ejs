<body class="is-preload">
	<div id="page-wrapper">

		<!-- Header -->
		<header id="header">
			<nav id="nav">
				<ul>
					<li><a href="/">Home</a></li>
					<li>
						<a href="#" class="icon solid fa-angle-down">Menu</a>
						<ul>
							<li><a href="/profile">Profile</a></li>
							<li><a href="/expensePage">Expenses</a></li>
							<li><a href="/livechatPage">Live Chat</a></li>
							<li><a href="/diningPage">Dining</a></li>
							<li><a href="/livechatPage">Rate Hospitals</a></li>
						</ul>
					</li>
					<li><a href="/signup" class="button">Sign Up</a></li>
				</ul>
			</nav>
		</header>

		<!-- Main -->
		<section id="main" class="container medium">
			<header>
				<h2>Hungry?</h2>
				<h3>Find restaurants in your area below .. </h3>
			</header>
			<div class="box">

				<form class="dining" method="post" action="/diningPage">
					<input class="zip" type="text" min="0" value="" name="Zipcode" placeholder="Enter Zipcode">
					<div class="col-12">

						<input class="food" type="submit" value="Find Locations">
					</div>
				</form>
			</div>

			<% if(restaurants) { %>
			<p>Here's a list of restaurants near you <%= zip %> </p>
			<ul id="list">
				<% for(let i=0; i < restaurants.length; i++) { %>
				<li class="restaurantsName">
					<% if(restaurants[i].restaurant_website == '') { %>
					<%=	restaurants[i].restaurant_name %>
					<% } else {  %>
					<a href="<%= restaurants[i].restaurant_website %>"> <%= restaurants[i].restaurant_name %> </a> <% } %> </li>
				<% } %>
			</ul>
			<% } %>

			<!-- </form> -->
	</div>

	</section>



